package datahiding1;

public class a {
    private String x = "hi";

    public String getx() {
        return this.x;
    }

    public void setx(String x) {
        this.x = x;
    }
}

package datahiding1;

public class b 
{
public static void main(String [] args)
{
a a1 = new a();
a1.setx("hello");
System.out.println(a1.getx());
}
}
